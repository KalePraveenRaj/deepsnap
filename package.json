{
  "name": "deepsnap",
  "productName": "DeepSnap",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-builder --win appx",
    "build": "electron-builder",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.1",
    "@electron-forge/maker-deb": "^7.6.1",
    "@electron-forge/maker-rpm": "^7.6.1",
    "@electron-forge/maker-squirrel": "^7.6.1",
    "@electron-forge/maker-zip": "^7.6.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.1",
    "@electron-forge/plugin-fuses": "^7.6.1",
    "@electron/fuses": "^1.8.0",
    "electron": "^34.1.1",
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "com.praveenraj.DeepSnap",
    "productName": "DeepSnap",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "appx",
        "nsis"
      ],
      "icon": "assets/icon.ico"
    },
    "appx": {
      "identityName": "DeepSnap",
      "publisher": "CN=Praveen Raj",
      "publisherDisplayName": "Praveen Raj",
      "applicationId": "DeepSnap"
    }
  },
  "keywords": [],
  "author": {
    "name": "Praveen Raj",
    "email": "kalepraveenraj@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.2",
    "electron-squirrel-startup": "^1.0.1",
    "esm": "^3.2.25",
    "fs": "^0.0.1-security",
    "openai": "^4.86.2",
    "path": "^0.12.7",
    "sharp": "^0.33.5",
    "tesseract.js": "^6.0.0"
  }
}
